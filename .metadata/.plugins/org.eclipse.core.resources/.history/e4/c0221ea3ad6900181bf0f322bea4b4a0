
public class Circle {

	private int radius;
	private double Radius;
	
	public Circle( int radius)
	{
		this.radius = radius;
	}
	public Circle(double radius)
	{
		this.Radius = radius;
	}
	
	
	public static void main(String[] args)
	{
		Circle myCircle = new Circle(10);
		System.out.println(myCircle.radius);
		
		System.out.println(myCircle.Radius);
		
	}
}
