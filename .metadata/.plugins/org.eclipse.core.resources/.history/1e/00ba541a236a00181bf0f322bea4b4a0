package codeFights;
import java.util.HashSet;
import java.util.Set;

public class firstDuplicaat {

		public static int firstDuplicate(int[] a) 
		 {
		    Set<Integer> mySet = new HashSet<Integer>();
		    int count = 0;
		    int firstIndex = 0;
		    int secondIndex = 0;
		    
		    for( int i = 0 ; i < a.length ; i++ )
		    {
		        if(!(mySet.add(a[i])) && count < 1)
		        {
		            firstIndex = i;
		            count++;
		        }
		        else if(!(mySet.add(a[i])) && count <= 1 )
		        {
		            secondIndex = i;
		            count++;
		            break;
		        }
		    }
		     
		     if( count == 1)
		     {
		         return a[firstIndex];
		     }
		     else if ( count == 2)
		     {
		         if( firstIndex < secondIndex)
		         {
		             return a[firstIndex];
		         }
		         else
		         {
		             return a[secondIndex];
		         }
		     }
		     else
		     {
		         return -1;
		     }
		}
		
		public static void main ( String[] args)
		{ 
			int a[] = {2, 1, 3, 5, 3, 2};
			System.out.println(firstDuplicate(a));
		}
		
		    
	}

