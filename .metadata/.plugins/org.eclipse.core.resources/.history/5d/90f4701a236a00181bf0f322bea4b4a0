
public class firstDuplicaat
 {
	int[][] rotateImage(int[][] a) {


    int[][] myArray = new int [a[0].length][a.length];
    
    for( int i = 0; i < a.length ; i++)
    {
        for( int j = 0; j < a[0].length ; j++)
        {
            myArray[a[0].length-j][a.length-i] = a[i][j];
        }
    }
    return myArray;
 }
}

